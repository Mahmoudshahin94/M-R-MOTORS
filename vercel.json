{
  "version": 2,
  "builds": [
    {
      "src": "mrmotors/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.11"
      }
    },
    {
      "src": "store/static/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/store/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "mrmotors/wsgi.py"
    }
  ],
  "env": {
    "DJANGO_SETTINGS_MODULE": "mrmotors.settings",
    "DEBUG": "False",
    "ALLOWED_HOSTS": ".vercel.app,m-r-motors.vercel.app",
    "VERCEL": "1"
  }
}
